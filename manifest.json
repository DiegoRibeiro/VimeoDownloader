{
	"name": "Private Vimeo Downloader",
	"version": "1.0",
	"description": "Plugin that allows download private video on vimeo",
	"manifest_version": 3,
	"background": {
		"service_worker": "background.js"
	},
	"permissions": [
		"activeTab",
		"scripting",
		"declarativeContent",
		"storage"
	],
	"action": {
		"default_popup": "src/popup.html"
	}
}